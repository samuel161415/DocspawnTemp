{
  "name": "dockspawn",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@package/typescript-config": "workspace:*",
    "eslint": "^8.56.0",
    "turbo": "latest"
  }
}
